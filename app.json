{
  "expo": {
    "name": "Thiên Phú Restaurant",
    "slug": "thienphu-restaurant",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.jpeg",
    "scheme": "thienphurestaurant",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "jsEngine": "hermes",
      "supportsTablet": true,
      "bundleIdentifier": "com.hongquan0312.myapp"
    },
    "android": {
      "enablePngCrunchInReleaseBuilds": false,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.jpeg",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json"
      },
      "package": "com.hongquan0312.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.jpeg"
    },
    "expo": {
      "jsEngine": "jsc"
    },
    "plugins": ["expo-router", "expo-font"],
    "extra": {
      "eas": {
        "projectId": "28697004-3e3e-4c24-b616-5dfca2e5764d"
      }
    }
  }
}
