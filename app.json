{
  "expo": {
    "name": "Thiên Phú Restaurant",
    "slug": "thienphu-restaurant",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/logoTP.jpg",
    "scheme": "thienphurestaurant",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "jsEngine": "hermes",
      "supportsTablet": true,
      "bundleIdentifier": "com.qk.backend.thienphurestaurant"
    },
    "android": {
      "enablePngCrunchInReleaseBuilds": false,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoTP.jpg",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json"
      },
      "package": "com.qk.backend.thienphurestaurant"
    },
    "web": {
      "favicon": "./assets/favicon.jpeg"
    },
    "expo": {
      "jsEngine": "jsc"
    },
    "plugins": ["expo-router", "expo-font"],
    "extra": {
      "eas": {
        "projectId": "4eddb6cb-3c68-4399-bae6-2fc546461f45"
      }
    },
    "owner": "qk.backend"
  }
}
